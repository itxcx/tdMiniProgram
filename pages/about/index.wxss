.module01 {
	position: relative;
	padding: 50rpx 40rpx 60rpx 40rpx;
	box-sizing: border-box;
}
.module01 .title {
	position: relative;
	margin-bottom: 50rpx;
}
.module01 .title .quote {
	width: 40rpx;
	height: 36rpx;
	position: absolute;
	top: 0;
}
.module01 .title .quote.l-quote {
	left: 0;
}
.module01 .title .quote.r-quote {
	right: 0;
	transform: rotate(180deg);
	-webkit-transform: rotate(180deg);
}
.module01 .title text {
  font-size: 60rpx;
  color: #333;
  line-height: 90rpx;
  text-align: center;
	font-weight: bold;
	width: 546rpx;
	margin: auto;
	display:block;
}
.module01 .module-content01 {
	text-align: justify;
	color: #333;
	font-size: 30rpx;
	line-height: 60rpx;
}
.module01 .module-content02 {
	text-align: justify;
	color: #333;
	font-size: 30rpx;
	line-height: 60rpx;
	margin-top: 40rpx;
	overflow: hidden;
	transition: height .5s;
	-webkit-transition: height .5s;
}
.module01 .toggleText {
	font-size: 0;
	text-align: center;
}
.module01 .toggleText text {
	font-size: 30rpx;
	line-height: 60rpx;
	color: #fab600;
	vertical-align: middle;
	display: inline-block;
}
.module01 .toggleText image {
	vertical-align: middle;
	display: inline-block;
	margin-left: 10rpx;
	width: 19rpx;
	height: 10rpx;
}
.module01 .toggleText image.up {
	transform: rotate(180deg);
	-webkit-transform: rotate(180deg);
}

